<div class="container">
  <div class="col-md-12 bg-secondary py-5 px-5">
    <div class="row align-items-center">
      <div class="pr-md-3 d-none d-md-block">
        <h4 class="text-primary">Search Exercises</h4>
      </div>
    </div>
    <div class="py-5 px-3">
      <form>
        <div class="form-row">
          {{! <div class="col form-group">
            <label for="from_date" class="form-label">From Date</label>
            <input
              type="date"
              name="from_date"
              class="form-control form-control-lg bg-dark text-white"
              placeholder="From Date"
            />
          </div> }}
          <div class="col form-group">
            <label for="to_date" class="form-label">To Date</label>
            <input
              type="text"
              id="searchedMuscle"
              name="muscle-group"
              class="form-control form-control-lg bg-primary text-white"
              placeholder="Enter a Muscle Group ('abs', 'biceps','quads')"
            />
          </div>
        </div>
        <div class="form-row">
          <div class="col">
            <input
              type="button"
              id="gifButton"
              class="btn btn-lg btn-block btn-dark border-light savebtn"
              value="Search"
            />
          </div>
        </div>
      </form>
      <div class="col bg-warning mt-3 text-center">
        <div class="exercise-container">
          <h2 id="exercise-name">Exercise Name: wide hand push up</h2>
          <h3 id="exercise-bodyPart">Main body part: chest</h3>
          <h3 id="exercise-target">Target muscle: pectorals</h3>
          <img
            id="exercise-giphy"
            src="http://d205bpvrqc9yn1.cloudfront.net/1311.gif"
            class="img-fluid"
            alt=""
          />
          <h3 id="exercise-equipment">Equipment: body weight</h3>

        </div>
      </div>
    </div>
  </div>
</div>
