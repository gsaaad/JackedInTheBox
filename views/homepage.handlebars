<div class="container-fluid position-relative bmi mb-5">
    <div class="container">
        <div class="row px-3 align-items-center">
            <div class="col-md-6">
                <div class="pr-md-3 d-none d-md-block">
                    <h4 class="text-primary">Workout Information</h4>
                    <h4 class="display-4 text-white font-weight-bold mb-4">Please Enter Workout Info</h4>
                </div>
            </div>
            <div class="col-md-6 bg-secondary py-5">
                <div class="py-5 px-3">
                    <form>
                        <div class="form-row">
                            <div class="col form-group">
                                <input type="text" class="form-control form-control-lg bg-dark text-white"
                                    placeholder="Name">
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="col form-group">
                                <input type="date" class="form-control form-control-lg bg-dark text-white"
                                    placeholder="Date">
                            </div>
                            <div class="col form-group">
                                <input type="text" class="form-control form-control-lg bg-dark text-white"
                                    placeholder="Goal">
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="col form-group">
                                <input type="text" class="form-control form-control-lg bg-dark text-white"
                                    placeholder="Sequence">
                            </div>
                            <div class="col form-group">
                                <input type="text" class="form-control form-control-lg bg-dark text-white"
                                    placeholder="Muscle Group">
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="col">
                                <input type="button" class="btn btn-lg btn-block btn-dark border-light savebtn" value="Save">
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
